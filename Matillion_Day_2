CREATE OR REPLACE WAREHOUSE DEMOWAREHOUSE;
CREATE DATABASE DEMODATABASE;
CREATE SCHEMA DEMO_SCHEMA;

CREATE OR REPLACE DATABASE DEMO_DB;
CREATE OR REPLACE SCHEMA STUDENTDATA;

CREATE TABLE StudentRecords(
RecordID INT PRIMARY KEY,
StudentName VARCHAR(100) NOT NULL,
Subject VARCHAR(100) NOT NULL,
Marks INT NOT NULL,
Year INT NOT NULL);

SELECT * FROM StudentRecords;
DELETE FROM StudentRecords;
SELECT * FROM DEMO_DB.STUDENTDATA."student_data_new";
DELETE  FROM DEMO_DB.STUDENTDATA."student_data_new";

CREATE OR REPLACE SCHEMA BANKING;
USE SCHEMA DEMO_SCHEMA;
DROP TABLE DEMODATABASE.DEMO_SCHEMA.TXNS_16;

CREATE OR REPLACE TABLE TRANSACTIONS(
trans_id INT,	
account_id	INT,
trans_date	DATE,
Type	VARCHAR(30),
operation	VARCHAR(40),
amount	INT,
balance	FLOAT,
Purpose	VARCHAR(40),
bank	VARCHAR(45),
account_partner_id INT);

SELECT * FROM TRANSACTIONS;

DELETE FROM TRANSACTIONS;
